{
    "name": "menchhub/filament-settings",
    "description": "A reusable settings package for Filament, powered by Spatie Laravel Settings.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.2",
        "filament/spatie-laravel-settings-plugin": "^3.2",
        "spatie/laravel-settings": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Menchhub\\FilamentSettings\\": "src/"
        }
    },
    "authors": [
        {
            "name": "lexistudio",
            "email": "menchhub@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Menchhub\\FilamentSettings\\SettingsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}